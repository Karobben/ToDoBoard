[calendars]


[[school]]
path = ~/.local/share/khal/calendars/sc
type = calendar
color = dark green

[[work]]
path = ~/.local/share/khal/calendars/work
type = calendar
color = yellow

[[private]]
path = ~/.local/share/khal/calendars/private
type = calendar
color = dark red

[[Daily Tasks]]
path = ~/.local/share/khal/calendars/dl
type = calendar
color = dark blue

[[Social]]
path = ~/.local/share/khal/calendars/scl
type = calendar
color = dark cyan

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M


[default]
highlight_event_days = True
